#include <stdio.h>
#include <stdlib.h>

int main()
{
    int x;
   printf("Please Enter your value of x:\n");
   scanf("%d", &x);

   if(x>18){
    printf("Congratulation! You are eligible for casting your vote.");
   }
   else{
    printf(" You are not eligible for casting your vote.");
   }
    return 0;
}
